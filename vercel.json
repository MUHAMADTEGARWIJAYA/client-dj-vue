{
  "rewrites": [
    {
      "source": "/api/:path*", 
      "destination": "https://server-dj-vue.vercel.app/api/:path"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
    
  ],
  
  "redirects": [
    { "source": "/", "destination": "/ps2", "permanent": true }
  ]

}
